<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="hibernate.bean">
 <class name="KeywordEntry" table="tb_keywordentry">
  <id column="id" name="id">
   <generator class="native"/>
  </id>
  <property column="keyword" generated="never" lazy="false"
   name="keyword" not-null="true" type="string"/>
  <property column="weigh" generated="never" lazy="false" name="weigh"
   not-null="true" type="integer"/>
 </class>
</hibernate-mapping>
